<% include partials/header %>

    <h1>
        <%= campground["name"] %>
    </h1>
    <img src="<%= campground["image"] %>" alt="Campground-image">
    <p>
        <%= campground["description"] %>
    </p>
    <% campground.comments.forEach (function (comment) { %>
        <p>
            <%= comment["text"] %>
        </p>
    <% }); %>

<% include partials/footer %>